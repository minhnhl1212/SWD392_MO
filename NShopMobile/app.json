{
  "expo": {
    "name": "NShopMobile",
    "slug": "NShopMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "my-scheme",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.nshopmobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "8b765e63-eeb9-4a5f-a0e5-88fac5699de7"
      }
    }
  }
}
